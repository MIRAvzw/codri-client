<?xml version="1.0"?>
 <scpd xmlns="urn:schemas-upnp-org:service-1-0">
     <specVersion>
         <major>1</major>
         <minor>0</minor>
     </specVersion>
     <actionList>
         <action>
             <name>Shutdown</name>
          </action>
          <action>
              <name>Reboot</name>
          </action>
		 <action>
			 <name>SetInterfaceLocation</name>
			 <argumentList>
				 <argument>
					 <name>iInterfaceLocationValue</name>
					 <relatedStateVariable>InterfaceLocation</relatedStateVariable>
					 <direction>in</direction>
				 </argument>
			 </argumentList>
		  </action>
		  <action>
		  	<name>LoadInterface</name>
		  </action>
		  <action>
		  	<name>GetInterfaceRevision</name>
			 <argumentList>
				 <argument>
					 <name>oInterfaceRevisionValue</name>
					 <relatedStateVariable>InterfaceRevision</relatedStateVariable>
					 <direction>out</direction>
				 </argument>
			 </argumentList>
		  </action>
      </actionList>
      <serviceStateTable>
		  <stateVariable sendEvents="no">
			  <name>InterfaceLocation</name>
			  <dataType>string</dataType>
			  <defaultValue></defaultValue>
		  </stateVariable>
		  <stateVariable sendEvents="yes">
			  <name>InterfaceRevision</name>
			  <dataType>ui4</dataType>
			  <defaultValue>0</defaultValue>
		  </stateVariable>
      </serviceStateTable>
 </scpd>
