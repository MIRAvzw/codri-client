<?xml version="1.0"?>
 <scpd xmlns="urn:schemas-upnp-org:service-1-0">
	 <specVersion>
		 <major>1</major>
		 <minor>0</minor>
	 </specVersion>
	 <actionList>
		 <action>
			 <name>SetInterface</name>
			 <argumentList>
				 <argument>
					 <name>iInterfaceValue</name>
					 <relatedStateVariable>Interface</relatedStateVariable>
					 <direction>in</direction>
				 </argument>
			 </argumentList>
		  </action>
		  <action>
		  	<name>GetInterfaceRevision</name>
			 <argumentList>
				 <argument>
					 <name>oInterfaceRevisionValue</name>
					 <relatedStateVariable>InterfaceRevision</relatedStateVariable>
					 <direction>out</direction>
				 </argument>
			 </argumentList>
		  </action>
		  <action>
			  <name>SetMedia</name>
			  <argumentList>
				  <argument>
					  <name>iMediaValue</name>
					  <relatedStateVariable>Media</relatedStateVariable>
					  <direction>in</direction>
				  </argument>
			  </argumentList>
		  </action>
		  <action>
		  	<name>GetMediaRevision</name>
			 <argumentList>
				 <argument>
					 <name>oMediaRevisionValue</name>
					 <relatedStateVariable>MediaRevision</relatedStateVariable>
					 <direction>out</direction>
				 </argument>
			 </argumentList>
		  </action>
	  </actionList>
	  <serviceStateTable>
		  <stateVariable sendEvents="no">
			  <name>Interface</name>
			  <dataType>string</dataType>
			  <defaultValue></defaultValue>
		  </stateVariable>
		  <stateVariable sendEvents="yes">
			  <name>InterfaceRevision</name>
			  <dataType>ui4</dataType>
			  <defaultValue>0</defaultValue>
		  </stateVariable>
		  <stateVariable sendEvents="no">
			  <name>Media</name>
			  <dataType>string</dataType>
			  <defaultValue></defaultValue>
		  </stateVariable>
		  <stateVariable sendEvents="yes">
			  <name>MediaRevision</name>
			  <dataType>ui4</dataType>
			  <defaultValue>0</defaultValue>
		  </stateVariable>
	  </serviceStateTable>
 </scpd>
