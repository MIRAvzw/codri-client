<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <actionList>
    <action>
      <name>LoadMedia</name>
      <argumentList>
        <argument>
          <name>iMediaIdentifierValue</name>
          <relatedStateVariable>MediaIdentifier</relatedStateVariable>
          <direction>in</direction>
        </argument>
        <argument>
          <name>iMediaLocationValue</name>
          <relatedStateVariable>MediaLocation</relatedStateVariable>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetMedia</name>
      <argumentList>
        <argument>
          <name>oMediaIdentifierValue</name>
          <relatedStateVariable>MediaIdentifier</relatedStateVariable>
          <direction>out</direction>
        </argument>
        <argument>
          <name>oMediaLocationValue</name>
          <relatedStateVariable>MediaLocation</relatedStateVariable>
          <direction>out</direction>
        </argument>
      </argumentList>
    </action>
  </actionList>
  <serviceStateTable>
    <stateVariable sendEvents="no">
      <name>MediaIdentifier</name>
      <dataType>string</dataType>
      <defaultValue></defaultValue>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>MediaLocation</name>
      <dataType>string</dataType>
      <defaultValue></defaultValue>
    </stateVariable>
  </serviceStateTable>
</scpd>
