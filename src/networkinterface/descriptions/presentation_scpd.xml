<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <actionList>
    <action>
      <name>GetRevision</name>
      <argumentList>
        <argument>
          <name>oRevisionValue</name>
          <relatedStateVariable>Revision</relatedStateVariable>
          <direction>out</direction>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetLocation</name>
      <argumentList>
        <argument>
          <name>oLocationValue</name>
          <relatedStateVariable>Location</relatedStateVariable>
          <direction>out</direction>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetLocation</name>
      <argumentList>
        <argument>
          <name>iLocationValue</name>
          <relatedStateVariable>Location</relatedStateVariable>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>
  </actionList>
  <serviceStateTable>
    <stateVariable sendEvents="no">
      <name>Revision</name>
      <dataType>ui4</dataType>
      <defaultValue></defaultValue>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>Location</name>
      <dataType>string</dataType>
      <defaultValue></defaultValue>
    </stateVariable>
  </serviceStateTable>
</scpd>
